<!DOCTYPE html>
<html>
  <head>
    <title>PatientRegistrationFrontEnd</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="content">
      <% unless flash[:notice].nil? %>
        <p id="flash_notice" style="color: green"><%= flash[:notice] %></p>
      <% end %>
      <% unless flash[:error].nil? %>
        <p id="flash_error" style="color: red"><%= flash[:error] %></p>
      <% end %>
      <%= yield %>
    </div>
    <script src="/touchscreentoolkit/lib/javascripts/touchScreenToolkit.js"></script>
  </body>
</html>