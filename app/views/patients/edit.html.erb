<h1 class="content-header">Edit Patient</h1>
<%= form_tag( 'path' , remote: true) do |f| %>
    <%= label_tag('person', 'Person:') %>
    <%= number_field_tag('person', value: @patients['person']['id']) %>
    <span class="helptext">Associate user with person identified by this id</span>

 
    <%= submit_tag %>
<% end %>

