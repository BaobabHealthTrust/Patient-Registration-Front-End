
<table class="listing" summary="Subject list">
  <tr class="header">
    <th></th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Gender</th>
    <th>D.O.B</th>
  </tr>

  <% @patients.each do |x| %>
  <tr>
    <td><%= x[:person_id] %></td>
    <td><%= x[:created_at] %></td>
    <td><%= x[:updated_at] %></td>


    <td class="actions">
      <%= link_to("Show", people_path(x[:person_id]), :class => 'action show') %>
      <%= link_to("Edit", '#', :class => 'action edit') %>
      <%= link_to("Delete", '#', :class => 'action delete') %>
    </td>
  </tr>
  <% end %>
</table>
